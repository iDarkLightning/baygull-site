{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "baygull-site",
  "main": ".output/server/index.mjs",
  "compatibility_date": "2025-09-10",
  "compatibility_flags": ["nodejs_compat", "nodejs_compat_populate_process_env", "enable_nodejs_http_modules"],
  "workers_dev": true,
  "assets": {
    "directory": ".output/public"
  },
  "observability": {
    "enabled": true
  }
}